<stylesheets>
	<stylesheet name="button">
		<background color="0xc6 0xc6 0xc6" hoverColor="0x94 0x94 0x94" pressColor="0x61 0x61 0x61" disableColor="0xd4 0xd4 0xd4" />
		<border size="-1" color="0x94 0x94 0x94" />
	</stylesheet>
    <stylesheet name="darkText">
        <text font="font" color="0x00 0x00 0x00" disableColor="0x77 0x77 0x77" />
    </stylesheet>
	<stylesheet name="costText">
		<text font="font" color="0xff 0xff 0xff" highlightColor="0xff 0x00 0x00" />
	</stylesheet>
	<stylesheet name="window">
		<bar color="0x4d 0x4d 0x4d" height="16" />
		<background color="0xc6 0xc6 0xc6" />
		<border size="1" color="0x94 0x94 0x94" />
		<title font="font" color="0xc6 0xc6 0xc6" characterSize="12" />
		<close color="0xe5 0x5e 0x30" highlightColor="0xea 0x6d 0x43" />
	</stylesheet>
    <stylesheet name="tabs">
        <background color="0xc6 0xc6 0xc6" />
    </stylesheet>
    <stylesheet name="windowTabs">
        <border size="-1" color="0x94 0x94 0x94" />
    </stylesheet>
    <stylesheet name="table">
        <title>
            <background color="0x77 0x77 0x77" />
            <text font="font" color="0x00 0x00 0x00" />
        </title>
        <cell>
            <background color="0xab 0xab 0xab" />
        </cell>
    </stylesheet>
    <stylesheet name="bar">
        <background color="0xc6 0xc6 0xc6" />
        <border size="-1" color="0x94 0x94 0x94" />
    </stylesheet>
    <stylesheet name="input">
		<background color="0xd4 0xd4 0xd4" />
        <text style="darkText" />
	</stylesheet>
</stylesheets>
