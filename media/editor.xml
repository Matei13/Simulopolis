<gui>
	<text name="selectionCostText" characterSize="12" style="text" />
	<widget name="rightMenu">
        <widget name="tabButtons">
            <button name="landscapeTabButton" size="32 32" style="button">
                <image name="grassTabImage" texture="grass" size="24 18" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <button name="housingTabButton" size="32 32" style="button">
                <image name="housingTabImage" texture="residential" rect="0 85 132 132" size="24 24" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <button name="industryTabButton" size="32 32" style="button">
                <image name="industryTabImage" texture="industrial" rect="0 85 132 132" size="24 24" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <button name="businessTabButton" size="32 32" style="button">
                <image name="businessTabImage" texture="commercial" rect="0 85 132 132" size="24 24" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <button name="healthTabButton" size="32 32" style="button">
                <image name="healthTabImage" texture="hospital" rect="0 85 132 132" size="24 24" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <button name="securityTabButton" size="32 32" style="button">
                <image name="securityTabImage" texture="police" rect="0 85 132 132" size="24 24" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <button name="educationTabButton" size="32 32" style="button">
                <image name="educationTabImage" texture="school" rect="0 85 132 132" size="24 24" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <button name="roadTabButton" size="32 32" style="button">
                <image name="roadTabImage" texture="road" rect="0 0 132 101" size="24 18" />
                <hboxlayout hAlignment="center" vAlignment="center" />
            </button>
            <vboxlayout hAlignment="left" vAlignment="top" />
        </widget>
        <tabwidget name="tabs">
            <widget name="landscapeTab" size="160 600" style="tabs">
                <widget name="landscapeTitle">
                    <text name="landscapeText" text="Landscape" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="landscapeButtons">
                    <button name="grassButton" size="32 32" style="button">
                        <image name="grassImage" texture="grass" size="24 18" />
                        <hboxlayout hAlignment="center" vAlignment="center" />
                    </button>
                    <button name="forestButton" size="32 32" style="button">
                        <image name="forestImage" texture="forest" size="24 18" />
                        <hboxlayout hAlignment="center" vAlignment="center" />
                    </button>
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <widget name="housingTab" size="160 600" style="tabs">
                <widget name="housingTitle">
                    <text name="housingText" text="Housing" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="housingButtons">
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <widget name="industryTab" size="160 600" style="tabs">
                <widget name="industryTitle">
                    <text name="industryText" text="Industry" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="industryButtons">
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <widget name="businessTab" size="160 600" style="tabs">
                <widget name="businessTitle">
                    <text name="businessText" text="Business" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="businessButtons">
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <widget name="healthTab" size="160 600" style="tabs">
                <widget name="healthTitle">
                    <text name="healthText" text="Health" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="healthButtons">
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <widget name="securityTab" size="160 600" style="tabs">
                <widget name="securityTitle">
                    <text name="securityText" text="Security" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="securityButtons">
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <widget name="educationTab" size="160 600" style="tabs">
                <widget name="educationTitle">
                    <text name="educationText" text="Education" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="educationButtons">
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <widget name="roadTab" size="160 600" style="tabs">
                <widget name="roadTitle">
                    <text name="roadText" text="Roads" characterSize="12" style="button" />
                    <hboxlayout hAlignment="center" />
                </widget>
                <widget name="roadButtons">
                    <button name="roadGrassButton" size="32 32" style="button">
                        <image name="roadGrassImage" texture="road" rect="0 0 132 101" size="24 18" />
                        <hboxlayout hAlignment="center" vAlignment="center" />
                    </button>
                    <button name="roadSidewalkButton" size="32 32" style="button">
                        <image name="roadSidewalkImage" texture="road" rect="132 0 132 101" size="24 18" />
                        <hboxlayout hAlignment="center" vAlignment="center" />
                    </button>
                    <button name="roadWaterButton" size="32 32" style="button">
                        <image name="roadWaterImage" texture="road" rect="264 0 132 101" size="24 18" />
                        <hboxlayout hAlignment="center" vAlignment="center" />
                    </button>
                    <hboxlayout hAlignment="center" vAlignment="center" spacing="2" />
                </widget>
                <vboxlayout hAlignment="center" vAlignment="top" margins="16 16 16 16" spacing="16" />
            </widget>
            <overlaplayout />
        </tabwidget>
		<hboxlayout hAlignment="right" vAlignment="top" />
	</widget>
    <widget name="infoBar">
		<button name="dayButton" size="160 16" style="button">
			<text name="dayText" characterSize="12" style="button" />
			<hboxlayout hAlignment="center" vAlignment="center" />
		</button>
		<button name="fundsButton" size="160 16" style="button">
			<text name="fundsText" characterSize="12" style="button" />
			<hboxlayout hAlignment="center" vAlignment="center" />
		</button>
		<button name="populationButton" size="160 16" style="button">
			<text name="populationText" characterSize="12" style="button" />
			<hboxlayout hAlignment="center" vAlignment="center" />
		</button>
		<button name="employmentButton" size="160 16" style="button">
			<text name="employmentText" characterSize="12" style="button" />
			<hboxlayout hAlignment="center" vAlignment="center" />
		</button>
		<button name="currentTileButton" size="160 16" style="button">
			<text name="currentTileText" characterSize="12" style="button" />
			<hboxlayout hAlignment="center" vAlignment="center" />
		</button>
		<hboxlayout vAlignment="bottom" />
	</widget>
</gui>
